<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="457.0" prefWidth="558.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="projects.oopfinalproject.FXMLCheckController">
   <children>
      <TitledPane alignment="CENTER" animated="false" prefHeight="26.0" prefWidth="559.0" text="Check In/Out" />
    <Button fx:id="buttonCheckIn" layoutX="90.0" layoutY="239.0" mnemonicParsing="false" onAction="#handleButtonCheckIn" prefHeight="30.0" prefWidth="164.0" text="Check In" />
      <Button fx:id="buttonSearchBooking" layoutX="308.0" layoutY="94.0" mnemonicParsing="false" onAction="#handleButtonSearchBooking" prefHeight="30.0" prefWidth="164.0" text="Search for Booking" />
      <Button fx:id="buttonCheckOut" layoutX="308.0" layoutY="240.0" mnemonicParsing="false" onAction="#handleButtonCheckOut" prefHeight="30.0" prefWidth="164.0" text="Check Out" />
      <Button fx:id="buttonExit" layoutX="308.0" layoutY="401.0" mnemonicParsing="false" onAction="#handleButtonExitAction" prefHeight="30.0" prefWidth="164.0" text="Exit" />
      <TextField layoutX="105.0" layoutY="96.0" promptText="Enter Booking ID" />
   </children>
</AnchorPane>
